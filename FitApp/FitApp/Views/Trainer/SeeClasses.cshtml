@model List<FitApp.Models.GymClass>
@{
    ViewBag.Title = "SeeClasses";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div>
    <table  class="table table-striped" style="width:100%">
        <thead>
            <tr>
                <th scope="col"> <strong>Name</strong></th>
                <th scope="col"><strong>Maximum Clients</strong></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var gym in Model)
            {
                <tr>
                    <td>@gym.Name</td>
                    <td>@gym.MaxClients</td>
                </tr>
            }
        </tbody>
    </table>

</div>

